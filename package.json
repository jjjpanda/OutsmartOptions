{
  "name": "outsmart_options",
  "version": "1.0.0",
  "description": "Y'all ever wanted to make money?",
  "main": "server.js",
  "scripts": {
    "start": "npx webpack && node server/server.js",
    "startContinuous": "npm-run-all -p createDistContinuous nodemonStart",
    "test": "npm-run-all -s testReact",
    "testReact": "jest test/reactTests --runInBand",
    "testEndpoints": "jest test/nodeTests --runInBand",
    "testCrossBrowser": "jest test/browserTests --runInBand --setupFiles dotenv/config",
    "testAll": "jest --setupFiles dotenv/config --runInBand",
    "lint": "eslint src/**/*.js server/**/*.js test/**/*.js > eslint/lintOutput.txt",
    "lintRemover": "eslint src/**/*.js server/**/*.js test/**/*.js --fix > eslint/lintOutput.txt",
    "createDistContinuous": "npx webpack --watch",
    "nodemonStart": "npx nodemon ./server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjjpanda/OutsmartOptions.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/jjjpanda/OutsmartOptions/issues"
  },
  "eslintIgnore": [
    "/src/jsLib/html2canvas.min.js"
  ],
  "homepage": "https://github.com/jjjpanda/OutsmartOptions#readme",
  "dependencies": {
    "antd": "^3.23.4",
    "bcryptjs": "^2.4.3",
    "body-scroll-lock": "^2.6.4",
    "core-js": "^3.2.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-custom-rules": "file:eslint",
    "eslint-plugin-jest": "^23.1.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "is-empty": "^1.2.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.14",
    "node-cron": "^2.0.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-instagram-embed": "^1.5.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-twitter-embed": "^3.0.3",
    "reactour": "^1.15.5",
    "recharts": "^1.7.1",
    "request": "^2.88.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "styled-components": "^4.4.1",
    "url-loader": "^2.3.0",
    "validator": "^12.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "browserstack-local": "^1.4.2",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "modulePaths": [
      "src",
      "test"
    ],
    "projects": [
      {
        "displayName": "React",
        "setupFilesAfterEnv": [
          "<rootDir>/test/reactTests/setup.js"
        ],
        "transform": {
          "^.+\\.js$": "babel-jest",
          "^.+\\.jsx$": "babel-jest"
        },
        "moduleNameMapper": {
          "\\.(css|less|sass|scss)$": "<rootDir>/test/styleMock.js",
          "\\.(png|jpg|gif|svg)$": "<rootDir>/test/fileMock.js"
        },
        "moduleFileExtensions": [
          "js",
          "json",
          "jsx",
          "",
          "node"
        ],
        "coveragePathIgnorePatterns": [
          "<rootDir>/node_modules/"
        ],
        "testPathIgnorePatterns": [
          "<rootDir>/test/nodeTests/",
          "<rootDir>/test/browserTests/"
        ]
      },
      {
        "displayName": "Server",
        "testRunner": "jest-circus/runner",
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
          "<rootDir>/node_modules/"
        ],
        "testPathIgnorePatterns": [
          "<rootDir>/test/reactTests/",
          "<rootDir>/test/browserTests/"
        ]
      },
      {
        "displayName": "Browser",
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
          "<rootDir>/node_modules/"
        ],
        "testPathIgnorePatterns": [
          "<rootDir>/test/reactTests/",
          "<rootDir>/test/nodeTests/"
        ]
      }
    ]
  }
}
